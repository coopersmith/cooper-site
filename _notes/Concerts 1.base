filters:
  and:
    - contains(property.category, "[[Concerts]]")
    - not(contains(file.name, "template"))
views:
  - type: table
    name: Table
    order:
      - Dates
      - file.name
      - Artists
      - Openers
      - Venue
    sort:
      - column: property.Dates
        direction: DESC
    columnSize:
      property.Dates: 201
